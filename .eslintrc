{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "plugin:react/recommended"],
  "plugins": [
      "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
   "globals": {
      "React": true
  },
  "rules": {
    "react/prefer-stateless-function": 1
  }
}
